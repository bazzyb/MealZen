{
  "expo": {
    "name": "MealZen",
    "slug": "MealZen",
    "version": "1.0.0",
    "orientation": "portrait",
    "##icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/mealzen-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#5f3dc4"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.MealZen"
    },
    "android": {
      "adaptiveIcon": {
        "##foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.MealZen"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "##favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/noto-serif/NotoSerif_400Regular.ttf",
            "node_modules/@expo-google-fonts/noto-sans/NotoSans_400Regular.ttf",
            "node_modules/@expo-google-fonts/noto-sans/NotoSans_700Bold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
